{
  "name": "digital-university-admin-portal",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "22.13.1"
  },
  "scripts": {
    "prepare": "panda codegen",
    "check:types": "tsc --noEmit",
    "lint": "next lint",
    "lintfix": "next lint --fix",
    "test": "tsc --noEmit && jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev --turbopack",
    "dev:local": "rm -rf ./build && npm run prepare && node -r dotenv/config --env-file=.env --env-file=.env.local src/server.js",
    "build": "rm -rf ./build && npm run lint && next build",
    "start": "NODE_ENV=production node src/server.js",
    "codegen": "graphql-codegen --config codegenConfig.ts",
    "localstack:start": "docker run -d --name digital-u-localstack -p 9000:4566 -p 4566:4566 -e SERVICES=\"s3,sqs\" -e EDGE_PORT_HTTP=4566 -e S3_PORT=9000 localstack/localstack && sleep 3 && aws --no-cli-pager --endpoint-url=http://localhost:4566 sqs create-queue --queue-name notifications --region us-east-1 && aws --no-cli-pager --endpoint-url=http://localhost:9000 s3 mb s3://gc-dev-assets --region us-gov-west-1",
    "localstack:stop": "docker stop digital-u-localstack && docker rm digital-u-localstack"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@aws-sdk/client-s3": "^3.431.0",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "@carbon/icons-react": "^11.29.0",
    "@carbon/react": "1.38.0",
    "@cerberus/icons": "npm:@jsr/cerberus__icons@0.14.2",
    "@cerberus/panda-preset": "npm:@jsr/cerberus__panda-preset@0.14.2",
    "@cerberus/react": "npm:@cerberus-design/react@0.14.2",
    "@cerberus/styled-system": "npm:@cerberus-design/styled-system@0.14.2",
    "@datapunt/matomo-tracker-react": "^0.5.1",
    "@digital-u/digital-ui": "2.1.22",
    "@digital-u/services": "1.4.30",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@tanstack/react-table": "^8.10.7",
    "@tanstack/react-virtual": "3.0.0-beta.68",
    "@types/react-avatar-editor": "^13.0.4",
    "apollo-upload-client": "^17.0.0",
    "axios": "^1.7.9",
    "chart.js": "^4.3.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "graphql": "^16.8.1",
    "jspdf": "^3.0.0",
    "jspdf-autotable": "^3.8.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "next": "15.2.0",
    "next-auth": "^4.23.1",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-avatar-editor": "^13.0.2",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "19.0.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.51.1",
    "react-loading-skeleton": "^3.5.0",
    "react-papaparse": "^4.4.0",
    "react-table": "^7.8.0",
    "react-use": "^17.5.1",
    "swr": "^2.2.5",
    "theme-ui": "^0.16.1"
  },
  "devDependencies": {
    "@emotion/jest": "11.11.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@jest/reporters": "^29.7.0",
    "@pandacss/dev": "^0.49.0",
    "@pandacss/preset-panda": "^0.49.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/carbon-components-react": "^7.55.2",
    "@types/draft-js": "^0.11.17",
    "@types/draftjs-to-html": "^0.8.4",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "dotenv": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sonarjs": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "mock-apollo-client": "^1.0.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "overrides": {
    "esbuild": "0.24.2",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4"
  }
}
